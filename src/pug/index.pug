doctype html
html(lang='en')

    head

        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(name='description', content='')
        meta(name='author', content='')

        title Dragon Street

        // Favicon
        link(rel='icon', type='image/x-icon', href='assets/favicon.ico')

        // Font Awesome icons (free version)
        script(src='https://use.fontawesome.com/releases/v5.15.4/js/all.js', crossorigin='anonymous')

        // Google fonts
        link(href='https://fonts.googleapis.com/css2?family=Ubuntu:wght@400;700&display=swap', rel='stylesheet', type='text/css')

        // Core theme CSS (includes Bootstrap)
        link(href='css/styles.css', rel='stylesheet')

    body#page-top

        // Navigation
        nav#mainNav.navbar.navbar-expand-lg.navbar-dark.fixed-top
            .container
                a.navbar-brand(href='#page-top')
                    img(src='assets/img/navbar-logo.png', alt='Dragon Street')
                button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarResponsive', aria-controls='navbarResponsive', aria-expanded='false', aria-label='Toggle navigation')
                    | Menu
                    i.fas.fa-bars.ms-1
                #navbarResponsive.collapse.navbar-collapse.justify-content-between
                    ul.navbar-nav.text-uppercase.mr-auto
                        li.nav-item
                            a.nav-link(href='#about') About
                        li.nav-item
                            a.nav-link(href='#roadmap') Roadmap
                        li.nav-item
                            a.nav-link(href='#portfolio') Rarity
                        li.nav-item
                            a.nav-link(href='#faq') FAQ
                    .my-3.my-lg-0
                        a.btn.btn-dark.btn-social.mx-2(href='https://twitter.com/DragonStreetNFT', target='_blank')
                            i.fab.fa-twitter
                        a.btn.btn-dark.btn-social.mx-2(href='https://www.instagram.com/dragonstreetnft', target='_blank')
                            i.fab.fa-instagram
                        a.btn.btn-dark.btn-social.mx-2(href='!#', target='_blank')
                            i.fab.fa-discord
        // Masthead
        header.masthead
            .container
                .row
                    .col-md-6
                        .masthead-giphy
                            img.rounded-circle.img-fluid(src='assets/img/dragon.gif', alt='...')
                    .col-md-6
                        .masthead-subheading Welcome!
                        .masthead-heading.text-uppercase Dragon Street
                        a.btn.btn-mint.btn-xl.text-uppercase(href='#mint') Mint a NFT

        // About
        section#about.page-section.py-3
            .row.px-1
                each val in [1, 2, 3, 4, 5, 6]
                    .col-lg-2.col-sm-2.col-xs-2.px-1
                        .portfolio-item
                            img.img-fluid(src='assets/img/portfolio/' + val + '.jpeg', alt='portfolio item')
            .container.my-5
                .text-center
                    h2.section-heading.text-uppercase About
                    h3.section-subheading.text-muted Dragon who!
                .row
                    .col-lg-8.mx-auto.text-center
                        h4.my-3 Ds is a collection of 10000 Dragon Street Dudes.
                        p.text Long long time ago, they were hanging around flaming Dragon Street and now, they are here to burn the Eth Blockhain with you
                        p.text-muted Lets get ready to join the adventure with 10000 unique individual Nft heroes
            .row.px-1
                each val in [7, 8, 9, 10, 11, 12]
                    .col-lg-2.col-sm-3.px-1
                        .portfolio-item
                            img.img-fluid(src='assets/img/portfolio/' + val + '.jpeg', alt='portfolio item')

